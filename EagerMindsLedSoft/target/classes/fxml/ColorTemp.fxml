                onAction="#OnBtnSetColorTempClick" prefHeight="23.0" prefWidth="102.0" text="%btnSetColorTemp"/>
        <ComboBox id="cbColorTemp" fx:id="cbColorTemp" layoutX="131.0" layoutY="22.0" prefWidth="150.0"/>
        <Label id="lbColorTemp" fx:id="lbColorTemp" layoutX="14.0" layoutY="26.0" text="%lbColorTemp"/>
        <Button id="btnGetColorTemp" fx:id="btnGetColorTemp" layoutX="483.0" layoutY="22.0" mnemonicParsing="false"
                onAction="#OnBtnGetColorTempClick" prefHeight="23.0" prefWidth="102.0" text="%btnGetColorTemp"/>
    </children>
    
</AnchorPane>
