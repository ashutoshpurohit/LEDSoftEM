<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="645.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eagerminds.eagermindsledsoft.pages.app.AppManagerPageController">
    <children>
        <Button id="btnUpdateSystem" fx:id="btnUpdateSystem" layoutX="14.0" layoutY="16.0" mnemonicParsing="false"
                onAction="#OnUpdateSystem" prefHeight="23.0" prefWidth="109.0" text="btnUpdateSystem"/>
        <Button id="btnUpdateSoftware" fx:id="btnUpdateSoftware" layoutX="191.0" layoutY="16.0" mnemonicParsing="false"
                onAction="#OnUpdateSoftware" prefHeight="23.0" prefWidth="109.0" text="btnUpdateSoftware"/>
        <Button id="btnInstallAPK" fx:id="btnInstallAPK" layoutX="372.0" layoutY="16.0" mnemonicParsing="false"
                onAction="#OnInstallAPK" prefHeight="23.0" prefWidth="109.0" text="btnInstallAPK"/>
        <TextArea id="taMsg" fx:id="taMsg" layoutX="14.0" layoutY="150.0" prefHeight="237.0" prefWidth="618.0"/>
        <TextField id="txtUninstallPackName" fx:id="txtUninstallPackName" layoutX="191.0" layoutY="63.0"
                   prefHeight="23.0" prefWidth="290.0"/>
        <Label id="lbUninstallPackName" fx:id="lbUninstallPackName" layoutX="14.0" layoutY="67.0"
               text="%uninstallPackName"/>
        <Button id="btnUninstallAPK" fx:id="btnUninstallAPK" layoutX="523.0" layoutY="63.0" mnemonicParsing="false"
                onAction="#OnUninstallAPK" prefHeight="23.0" prefWidth="109.0" text="%btnUninstallAPK"/>
    </children>
</AnchorPane>